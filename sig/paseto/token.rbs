module Paseto
  class Token
    include Comparable

    attr_reader version: String
    attr_reader purpose: String
    attr_reader payload: String
    attr_reader footer: String
    
    def self.parse: (String str) -> Token
    def header: -> String
    def to_s: -> String
    def <=>: (untyped) -> (Integer | nil)    

    private

    def initialize: (payload: String, purpose: String, version: String, ?footer: String) -> void
    def valid?: -> bool
  end
end
